<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote name="github"
            fetch="https://github.com"/>

    <remote name="shoesensor"
            fetch="https://github.com/ShoeSensor"
            revision="sdk11"/>

    <default revision="sdk11"
             remote="github"
             sync-c="true"
             sync-j="4" />

    <project name="nrf_mma8453q"
             path="apps/nrf_mma8453q"
             remote="shoesensor"/>

    <project name="nrf_ble"
             path="apps/nrf_ble"
             remote="shoesensor"/>

    <project name="nrf_freertos"
             path="apps/nrf_freertos"
             remote="shoesensor"/>

    <project name="nrf_datafilter"
             path="apps/nrf_datafilter"
             remote="shoesensor"/>

    <project name="apps_angle_bench"
             path="apps/angle_bench"
             remote="shoesensor"/>

    <project name="nrf_uart"
             path="apps/nrf_uart"
             remote="shoesensor"/>

    <project name="android_demo"
             path="apps/RunningShoeSensor"
             remote="shoesensor"/>

    <project name="nrf_sdk"
             path="./"
             remote="shoesensor"/>

    <project name="manifest"
             path="manifest"
             remote="shoesensor"/>

</manifest>
